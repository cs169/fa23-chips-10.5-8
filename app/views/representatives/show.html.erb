.container-fluid
    .row
        .col-md-8.offset-md-2
            .table-responsive-md.pt-5
                %table.table.table-striped.table-hover#events
                    %caption Table of Representatives
                    %thead.thead-dark
                        %tr
                            %th Street
                            %th City
                            %th State
                            %th Zip
                            %th Political Party
                            %th Photo
                    %tbody
                        - @representatives.each_with_index do |rep, _index|
                            %tr
                                %td= rep.street
                                %td= rep.city
                                %td= rep.state
                                %td= rep.zip
                                %td= rep.political_party
                                %td= rep.photo
