.container-fluid
    .row
        .col-md-8.offset-md-2
            .table-responsive-md.pt-5
                %table.table.table-striped.table-hover#events
                    %caption Table of Representatives
                    %thead.thead-dark
                        %tr
                            %th Street
                            %th City
                            %th State
                            %th Zip
                            %th Political Party
                            %th Photo
                    %tbody
                        %tr
                            %td= @representative.street
                            %td= @representative.city
                            %td= @representative.state
                            %td= @representative.zip
                            %td= @representative.political_party
                            %td 
                                %img{ src: @representative.photo, alt: "Representative Photo" }

